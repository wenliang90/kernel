#!/usr/bin/make -f

srctree ?= .

build:
	$(MAKE) KERNELRELEASE=5.4.46-rockchip64 ARCH=arm64 	KBUILD_BUILD_VERSION=trunk -f $(srctree)/Makefile

binary-arch:
	$(MAKE) KERNELRELEASE=5.4.46-rockchip64 ARCH=arm64 	KBUILD_BUILD_VERSION=trunk -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	$(MAKE) clean

binary: binary-arch
